{
  "expo": {
    "name": "Chitfund", // ‚úÖ Good
    "slug": "Chitfund", 
    "version": "1.0.1", 
    "orientation": "portrait",
    "icon": "./assets/icon.png", // ‚úÖ Good (make sure file exists)
    "scheme": "chitfund", // ‚úÖ Good
    "userInterfaceStyle": "automatic",
    // "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 3, // üîß Increment (2 ‚Üí 3) for a new APK/Play Store update
      "package": "com.sibi5161.chitfund", // üîß Change to lowercase (Android best practice)
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#E6F4FE"
        // ‚ùå Remove backgroundImage & monochromeImage if they are same as foreground
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "acb5f68a-dfd8-4f56-8c2e-44b28d69686c"
      }
    }
  }
}
